DÃ©but programme
    VARIABLES LOCALES :
        Var tab :tab[23]
        Var TAILLE :N <- 23
        Var i :N <- 1
        Var j :N <- 1
    FIN VARIABLES LOCALES

    tab <- ['S', 'U', 'P', 'P', 'R', 'I', 'M', 'E', 'R', ' ', ' ', 'L', 'E', 'S', ' ', 'E', 'S', 'P', 'A', 'C', 'E', 'S', ' ']

    i <- 1
    TANTQUE i <= 23
        SI tab[i] = ' ' ALORS
            j <- i
            TANTQUE i < TAILLE - 1
                tab[j] <- tab[j+1]
                j <- j+ 1
            FINTANTQUE

            tab[TAILLE] <- ' '
        FIN SI
        i <- i + 1
    FINTANTQUE
Fin programme